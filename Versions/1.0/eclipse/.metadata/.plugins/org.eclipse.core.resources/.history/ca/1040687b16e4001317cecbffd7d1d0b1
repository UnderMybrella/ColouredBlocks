package org.abimon.mods.minecraft.dnc;

import java.util.HashMap;

import net.minecraft.client.Minecraft;
import net.minecraft.item.Item;
import net.minecraft.server.MinecraftServer;

public class WeightThread extends Thread 
{
	public static HashMap<Item, Integer> WEIGHT = new HashMap<Item, Integer>();
	
	public void run()
	{
		while(true)
		{
			MinecraftServer server = MinecraftServer.getServer();
			for(String name : server.getAllUsernames())
			{
				server.worldServerForDimension(0)name;
			}
		}
	}
}
